		scale: 0.5
	},
	default-font-chat: {
		file: square.fnt,
		markupEnabled: false,
		scale: 0.5
	},
	korean: {
		file: korean.fnt,
		markupEnabled: true,
		scale: 0.5
	},
	trad-chinese: {
		file: trad_chinese.fnt,
		markupEnabled: true,
		scale: 0.5
	},
	simp-chinese: {
		file: simp_chinese.fnt,
		markupEnabled: true,
		scale: 0.5
	},
	title: {
		file: title.fnt,
		markupEnabled: true,
		scale: 2
	}
},
Color: {
	black: {a: 1, b: 0, g: 0, r: 0 },
	white: {a: 1, b: 1, g: 1, r: 1 },
	gray: {a: 1, b: 0.32, g: 0.32, r: 0.32 },
    lightgray: {a: 1, b: 0.65, g: 0.65, r: 0.65 }
	orange: {hex: "FFA500"},
	accent: {hex: "f4ba6e"},
	accentDark: {hex: "f4ba6e"},
},
TintedDrawable: {
	dialogDim: {name: white, color: {r: 0, g: 0, b: 0, a: 0.9} },
	invis: {name: white, color: {r: 0, g: 0, b: 0, a: 0} }
